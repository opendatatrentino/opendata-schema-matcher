<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-Deploy to sonatype</actionName>
        <displayName>Deploy to sonatype</displayName>
        <goals>
            <goal>clean</goal>
            <goal>deploy</goal>
        </goals>
    </action>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <properties>
            <skipTests>true</skipTests>
        </properties>
    </action>
    <action>
        <actionName>rebuild</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>clean</goal>
            <goal>install</goal>
        </goals>
        <properties>
            <skipTests>true</skipTests>
        </properties>
    </action>
</actions>
